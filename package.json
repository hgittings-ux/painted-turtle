{
  "name": "painted-turtle",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3006 next dev",
    "dev:3006": "PORT=3006 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "free:3006": "lsof -i :3006 | grep LISTEN | awk '{print $2}' | xargs kill -9 || true",
    "dev:reset": "npm run free:3006 && npm run dev"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "next": "^13.4.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/node": "24.0.14",
    "@types/react": "19.1.8",
    "typescript": "5.8.3"
  }
}
